{
  "AzureAdB2C": {
    "Instance": "https://<tenant-name>.b2clogin.com/tfp/",
    "ClientId": "<client-id>",
    "Domain": "<tenant-name>.onmicrosoft.com",
    "PolicyId": "<policy-id>",
    "Scopes": {
      "IdentityRead": "Identity.Read",
      "IdentityReadWrite": "Identity.ReadWrite"
    },
    "Roles": {
      "IdentityReader": "Identity.Reader"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}